<extension>
    <id>d_social_like</id>
    <name>Social like</name>
    <description>
    This module adds social network like buttons to your webshop. place them anywhere you like and the like buttons will automatically like the current page.
    </description>
    <type>module</type>
    <version>2.0</version>
    <opencart_version>2.0.0.0;2.0.1.1;2.0.2.0;2.0.3.1;2.1.0.0;2.2.0.0</opencart_version>
    <mbooth_version>3.0</mbooth_version>
    <author>Dreamvention (Artem)</author>
    <files>
        <file>system/mbooth/xml/mbooth_d_social_like.xml</file>
		<file>system/config/d_social_like.php</file>
		<file>system/config/d_social_like_metro.php</file>

        <file>admin/controller/module/d_social_like.php</file>
		<file>admin/language/english/module/d_social_like.php</file>
		<file>admin/language/en-gb/module/d_social_like.php</file>
		<file>admin/model/module/d_social_like.php</file>
		<file>admin/view/template/module/d_social_like.tpl</file>
		<file>admin/view/stylesheet/d_social_like.css</file>
		
		<file>catalog/controller/module/d_social_like.php</file>
		<file>catalog/language/english/module/d_social_like.php</file>
		<file>catalog/language/en-gb/module/d_social_like.php</file>
		<file>catalog/view/theme/default/template/module/d_social_like.tpl</file>
    </files>
	<dirs>
		<dir>admin/view/image/d_social_like</dir>
		<dir>catalog/view/theme/default/stylesheet/d_social_like</dir>
	</dirs>
    <required>
        <require version="2.1.0">shopunity</require>
    </required>
    <updates>
        <update version="2.0"></update>
    </updates>
</extension>
